{
  "name": "babel-plugin-transform-react-pug",
  "version": "4.1.0",
  "description": "A plugin for transpiling pug templates to jsx",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "keywords": [
    "babel-plugin"
  ],
  "dependencies": {
    "babylon": "6.18.0",
    "common-prefix": "^1.1.0",
    "pug-error": "^1.3.2",
    "pug-filters": "^3.0.0",
    "pug-lexer": "^4.0.0",
    "pug-parser": "^4.0.1",
    "pug-strip-comments": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-forbeslindesay": "^2.1.0",
    "babel-types": "^6.24.1",
    "flow-bin": "^0.66.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "lint-staged": "^7.0.0",
    "prettier": "1.11.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "build": "babel-node scripts/generate-babel-types && babel-node scripts/build-visitors && babel src --out-dir lib",
    "flow": "flow",
    "lint": "prettier --list-different **/*.js",
    "test": "jest --coverage",
    "watch": "jest --coverage --watch",
    "format": "prettier --write **/*.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "src/__tests__/.*(\\.test.js)$",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/__tests__/*.js",
      "!src/babel-types.js"
    ],
    "snapshotSerializers": [
      "./scripts/filename-serializer.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pugjs/babel-plugin-transform-react-pug.git"
  }
}
