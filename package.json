{
  "name": "babel-plugin-transform-react-pug",
  "version": "3.1.8",
  "description": "A plugin for transpiling pug templates to jsx",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "keywords": [
    "babel-plugin"
  ],
  "dependencies": {
    "babylon": "^6.17.1",
    "pug-error": "^1.3.2",
    "pug-filters": "^2.1.2",
    "pug-lexer": "^3.1.0",
    "pug-parser": "^3.0.0",
    "pug-strip-comments": "1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-forbeslindesay": "^2.1.0",
    "babel-types": "^6.24.1",
    "eslint": "^1.10.3",
    "eslint-config-forbeslindesay": "^1.0.0",
    "flow-bin": "^0.47.0",
    "jest": "^20.0.4",
    "react": "^15.5.4",
    "react-test-renderer": "^15.5.4"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel-node scripts/generate-babel-types && babel-node scripts/build-visitors && babel src --out-dir lib",
    "flow": "flow",
    "lint": "eslint src",
    "test": "jest --coverage",
    "watch": "jest --coverage --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "src/__tests__/.*(\\.test.js)$",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/__tests__/*.js",
      "!src/babel-types.js"
    ],
    "snapshotSerializers": [
      "./scripts/filename-serializer.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pugjs/babel-plugin-transform-react-pug.git"
  }
}
